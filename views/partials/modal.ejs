<div
  class="modal fade"
  id="<%= modalId %>"
  tabindex="-1"
  aria-labelledby="projectDescriptionLabel"
  aria-hidden="true"
>
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="projectDescriptionLabel">
            <%= projectName %>
          </h5>
          <button
            type="button"
            class="close"
            data-dismiss="modal"
            aria-label="Close"
          >
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
            <h5>Outcome:</h5>
            <p><%= projectDescription %></p>
            <h5>Role:</h5>
            <p><%- projectRole %></p>
        </div>
      </div>
    </div>
  </div>
</div>
